{
  "name": "avataaars",
  "version": "0.1.0",
  "description": "Avataaars as a API",
  "main": "build/index.js",
  "scripts": {
    "start": "babel -d ./build src -s --watch",
    "build": "rm -rf ./build && mkdir ./build && babel -d ./build ./src",
    "test": "jest",
    "test:coverage": "jest --collect-coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ItsKerolos/avataaars.git"
  },
  "author": "Kerolos Zaki",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ItsKerolos/avataaars/issues"
  },
  "homepage": "https://github.com/ItsKerolos/avataaars#readme",
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "jest-extended"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@types/jest": "^25.1.4",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^25.1.0",
    "jest-extended": "^0.11.5"
  }
}
